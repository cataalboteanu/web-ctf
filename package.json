    "mongoose": "^6.2.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "pug": "^3.0.2",
    "sequelize": "5.8.9"
  },
  "devDependencies": {
    "compression": "^1.7.4",
    "nodemon": "^2.0.12",
    "standard": "^16.0.4"
  }
